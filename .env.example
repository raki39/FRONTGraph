# API Keys - Obrigatórias
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# LangSmith (Opcional - para observabilidade)
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_PROJECT=agentgraph-project

# CORS (Opcional - padrão já configurado)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Celery (Opcional - padrões já configurados)
USE_CELERY=true
REDIS_URL=redis://redis:6379/0
REDIS_RESULT_BACKEND=redis://redis:6379/0
CELERY_DEBUG=false
CELERY_ENABLED=true
CELERY_WORKER_PREFETCH_MULTIPLIER=1
CELERY_TASK_MAX_RETRIES=3
CELERY_TASK_DEFAULT_RETRY_DELAY=60
