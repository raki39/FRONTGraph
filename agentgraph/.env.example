# AgentGraph - Configurações Docker

# API Keys (OBRIGATÓRIO - Configure pelo menos uma)
OPENAI_API_KEY=sk-your_openai_key_here
ANTHROPIC_API_KEY=sk-ant-your_anthropic_key_here
GOOGLE_API_KEY=your_google_api_key_here
HUGGINGFACE_API_KEY=hf_your_huggingface_key_here

# LangSmith (OPCIONAL - para observabilidade)
LANGSMITH_API_KEY=lsv2_pt_your_langsmith_key_here
LANGSMITH_TRACING=true
LANGSMITH_PROJECT=agentgraph-docker

# Configurações do Gradio
GRADIO_SHARE=true
GRADIO_PORT=7860

# Configurações da aplicação
DEFAULT_MODEL=GPT-4o-mini
MAX_ITERATIONS=40
TEMPERATURE=0
LOG_LEVEL=INFO

# Configurações do Celery (OPCIONAL - processamento assíncrono)
CELERY_ENABLED=true
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0
CELERY_WORKER_CONCURRENCY=1
FLOWER_PORT=5555
